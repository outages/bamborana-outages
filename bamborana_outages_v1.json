    {
      "id": "1fv79zq0ff73",
      "name": "Incorrect Merchant Descriptors",
      "status": "identified",
      "created_at": "2024-01-04T12:29:06.587-08:00",
      "updated_at": "2024-01-11T08:53:54.958-08:00",
      "monitoring_at": null,
      "resolved_at": null,
      "impact": "minor",
      "shortlink": "https://stspg.io/r3blt2k1spl7",
      "started_at": "2024-01-04T12:29:06.580-08:00",
      "page_id": "dm5h7g8csh93",
      "incident_updates": [
        {
          "id": "psxf1rlbf5n5",
          "status": "identified",
          "body": "While investigating we discovered that in some instances a different business name was appearing on customer statements.  This issue was flagged and a fix was put into place by our upstream processor.\n\nDepending on the card issuer, some cardholders may see a generic descriptor like \"Bambora Internet Wholesale\" or “Bambora Internet PSP”.\n\nWe continue to work with our upstream processor to reduce the occurrences of this issue.",
          "incident_id": "1fv79zq0ff73",
          "created_at": "2024-01-11T08:53:54.955-08:00",
          "updated_at": "2024-01-11T08:53:54.955-08:00",
          "display_at": "2024-01-11T08:53:54.955-08:00",
          "affected_components": [
            {
              "code": "tpvhhtslhvh3",
              "name": "Acquirers - First Data",
              "old_status": "degraded_performance",
              "new_status": "degraded_performance"
            }
          ],
          "deliver_notifications": true,
          "custom_tweet": null,
          "tweet_id": null
        },
        {
          "id": "djv41jn5k5b4",
          "status": "investigating",
          "body": "We have identified an issue with our upstream processor where the DBA (Doing Business As) name is not properly appearing on transactions on some accounts.\n\nAs a result, the business name may not appear correctly on some cardholder statements.\n\nWe will provide updates once the solution has been confirmed.",
          "incident_id": "1fv79zq0ff73",
          "created_at": "2024-01-04T12:29:06.653-08:00",
          "updated_at": "2024-01-04T12:29:06.653-08:00",
