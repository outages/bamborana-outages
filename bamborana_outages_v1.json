      "incident_updates": [
        {
          "id": "w1jbkj2qsx0z",
          "status": "monitoring",
          "body": "Our teams have successfully re-populated the majority of the transaction data, and this is now available for viewing.\nIf any further transaction data discrepancies are found, during this time period, please reach out to our Customer Care team with the details: support.na@worldline.com",
          "incident_id": "snf77d3x83xr",
          "created_at": "2025-07-29T11:02:42.095-07:00",
          "updated_at": "2025-07-29T11:02:42.095-07:00",
